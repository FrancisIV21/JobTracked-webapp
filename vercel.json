{
  "rewrites": [
    {
      "source": "/JobTrackerDashboard",
      "destination": "/JobTrackerDashboard.html"
    },
    {
      "source": "/JobTrackerSignUp", 
      "destination": "/JobTrackerSignUp.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}